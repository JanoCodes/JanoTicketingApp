{
  "expo": {
    "name": "Jano Ticketing",
    "description": "Companion app to the Jano Ticketing System.",
    "slug": "jano-ticketing",
    "privacy": "unlisted",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/jano-may-ball/mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "jano",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "uk.co.janoticketing.mobile",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets."
      }
    },
    "android": {
      "package": "uk.co.janoticketing.mobile"
    },
    "androidStatusBar": {
      "barStyle": "dark-content"
    }
  }
}
